{
  "name": "CerasusTempor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean": "cd android && ./gradlew clean",
    "android:release": "react-native run-android --variant=release",
    "ios": "react-native run-ios --device '矢崎良和のiPhone'",
    "reconnect": "adb reverse tcp:8081 tcp:8081",
    "createBundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "buildBundle": "curl \"http://localhost:8081/index.bundle?platform=android\" -o \"android/app/src/main/assets/index.android.bundle\"",
    "assembleDebug": "cd android && ./gradlew assembleDebug",
    "assembleRelease": "cd android && ./gradlew assembleRelease",
    "createBundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint app/",
    "lint:fix": "eslint app/ --fix",
    "buildApk": "npm run clean && npm run createBundle && npm run buildBundle && npm run assembleRelease"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.4",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-hooks": "^0.9.0",
    "react-native-modal": "^11.5.6",
    "react-native-permissions": "^2.1.5",
    "react-native-responsive-fontsize": "^0.4.3",
    "react-native-safe-area-context": "^3.0.7",
    "react-native-screens": "^2.9.0",
    "react-native-vector-icons": "^7.0.0",
    "react-redux": "^7.2.0",
    "redux-actions": "^2.6.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "eslint-plugin-redux-saga": "^1.1.3",
    "jest": "^26.1.0",
    "metro-react-native-babel-preset": "^0.60.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
